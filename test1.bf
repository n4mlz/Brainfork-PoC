; 2 スレッドで同じセルを安全にインクリメント

[-]                     ; cell0 = 0

{                       ; 並列開始（2スレッド）
  (                     ; lock cell0
    ; PRINT 'A'
    +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++.
    [-]                 ; cell0 をクリア
  )                     ; unlock cell0
|
  (                     ; lock cell0
    ; PRINT 'B'
    ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++.
    [-]                 ; cell0 をクリア
  )                     ; unlock cell0
}
